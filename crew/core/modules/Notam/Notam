<?php
//TheRedsVirtual Notam Page Module

class Events extends CodonModule
{
    public function index()
    {
            if(!Auth::LoggedIn()) {
          $this->set('message', 'You must be logged in to access this feature!');
          $this->render('core_error.php');
          return;
        }
        $this->show('notam.php');
    }
}